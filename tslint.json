{
  "defaultSeverity": "error",
  "extends": "./node_modules/tslint/lib/configs/recommended",
  "jsRules": {},
  "rules": {
    "max-classes-per-file": [true, 2],
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "quotemark": false,
    "no-console": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "**/*.spec.ts",
      "node_modules/"
    ]
  }
}